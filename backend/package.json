{
  "name": "goplan-server",
  "version": "0.0.0",
  "description": "A financial planning platform for the general public.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoPlan-Finance/GoPlan-app.git"
  },
  "author": "GoPlan-Finance team",

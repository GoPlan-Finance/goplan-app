    "kerberos": "1.1.5",
    "module-alias": "^2.2.2",
    "mongodb": "^4.3.0",
    "netmask": ">=2.0.1",
    "parse": "3.4.1",
    "parse-server": "https://github.com/GoPlan-Finance/parse-server#master",
    "ts-money": "^0.4.6",
    "ts-node": "^10.2.1"
  },
  "devDependencies": {
    "@types/concurrently": "^7.0.0",
    "@types/convict": "^6.1.1",
    "@types/crypto-js": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.0",
    "@types/parse": "^2.18.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "babel-eslint": "10.1.0",
    "babel-watch": "7.7.0",
    "bluebird": "3.7.2",
    "concurrently": "^7.0.0",
    "eslint": "^8.1.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.25.4",

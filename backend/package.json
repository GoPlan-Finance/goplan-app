  "scripts": {
    "pre-commit": "yarn run lint && yarn run prettier",
    "lint": "eslint --cache --fix . --ext .js,.ts,.vue",
    "prettier": "prettier --write  src/ spec/",
    "backend-test": "jest",
    "backend-serve": "ts-node -r tsconfig-paths/register src/server.ts",

    "eslint-plugin-varspacing": "^1.2.2",
    "jest": "^27.0.6",
    "mongodb-runner": "4.8.3",
    "nodemon": "^2.0.13",
    "nyc": "15.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "redis": ">=3.1.1"
  }
}

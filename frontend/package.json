{
  "name": "goplan-frontend",
  "version": "0.0.0",
  "description": "a financial planning platform for the general public.",
  "main": "index.js",
  "scripts": {
    "pre-commit": "yarn run lint && yarn run prettier && yarn run yml-sort",
    "lint-fix": "eslint --cache --fix . --ext .js,.ts,.vue",
    "prettier": "prettier --write  src/ spec/",
    "test": "jest",
    "dev": "vite",

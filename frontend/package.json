    "@types/node": "^16.6.2",
    "@types/papaparse": "^5.2.6",
    "@types/parse": "^2.18.6",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.2.4",
    "autoprefixer": "^10.3.1",
    "concurrently": "^6.1.0",

    "lint": "eslint --cache . --ext .js,.ts,.vue",
    "yml-sort": "yml-sorter -i src/locales/en.yml -o src/locales/en.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoPlan-Finance/GoPlan-app.git"
  },
  "keywords": [
